{
  "expo": {
    "name": "Lean Fitness",
    "slug": "lean-fitness",
    "owner": "kushenthimira",
    "privacy": "unlisted",
    "platforms": ["ios", "android"],
    "version": "1.0.0-dev",
    "orientation": "portrait",
    "icon": "./assets/images/logo-sm.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "enabled": true,
      "fallbackToCacheTimeout": 10000
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "npc.cipher.leanfitness",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Use the photo library to change your avatar."
      },
      "userInterfaceStyle": "automatic"
    },
    "android": {
      "package": "npc.cipher.leanfitness",
      "versionCode": 1,
      "userInterfaceStyle": "automatic",
      "permissions": ["VIBRATE", "CAMERA"]
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": ["js", "jsx", "svg", "ts", "tsx"]
    },
    "description": "",
    "extra": {
      "eas": {
        "projectId": "9bdddd60-4660-495d-8796-e203b10ac528"
      }
    }
  }
}
